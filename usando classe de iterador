class MeuIterador:
    def __init__(self, numeros: list):
        self.numeros = numeros
        self.contador = 0

    def __iter__(self):
        return self

    def __next__(self):
        try:
              valor = self.numeros[self.contador]
              self.contador += 1
              return valor + 2
        except IndexError:
              raise StopIteration
# Definindo as vari√°veis
for i in MeuIterador([1, 2, 3, 4, 5]):
    print(i)
